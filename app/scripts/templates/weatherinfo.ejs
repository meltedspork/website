<div id="weatherInfo">
  <div class="row">
    <div class="col-md-10">
    <h1><%= weather.fahrenheit.temp %><sup><small>&deg;F</small></sup></h1>
    <h1><%= weather.description %></h1>
      <div class="row">
        <div class="col-sm-2">
          <h2><span class="glyphicon glyphicon-arrow-up"></span>
          <%= weather.fahrenheit.temp %><sup><small>&deg;F</small></sup></h2>
        </div>
        <div class="col-sm-2">
          <h2><span class="glyphicon glyphicon-arrow-down"></span>
          <%= weather.fahrenheit.temp %><sup><small>&deg;F</small></sup></h2>
        </div>
      </div>
    </div>
  </div>
</div>